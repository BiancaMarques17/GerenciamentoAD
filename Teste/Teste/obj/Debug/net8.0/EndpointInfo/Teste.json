{
  "openapi": "3.0.1",
  "info": {
    "title": "Teste",
    "version": "1.0"
  },
  "paths": {
    "/api/ad/create-user": {
      "post": {
        "tags": [
          "ActiveDirectory"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UserDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UserDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/ad/organizational-units": {
      "get": {
        "tags": [
          "ActiveDirectory"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/ad/list-groups": {
      "get": {
        "tags": [
          "ActiveDirectory"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "UserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "surname": {
            "type": "string",
            "nullable": true
          },
          "nomeCompleto": {
            "type": "string",
            "nullable": true
          },
          "departamento": {
            "type": "string",
            "nullable": true
          },
          "login": {
            "type": "string",
            "nullable": true
          },
          "telefone": {
            "type": "string",
            "nullable": true
          },
          "cpf": {
            "type": "string",
            "nullable": true
          },
          "cargo": {
            "type": "string",
            "nullable": true
          },
          "organizationalUnit": {
            "type": "string",
            "nullable": true
          },
          "grupos": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "senhaNuncaExpira": {
            "type": "boolean"
          },
          "contaDesabilitada": {
            "type": "boolean"
          },
          "dataExpiracao": {
            "type": "integer",
            "format": "int64"
          }
        },
        "additionalProperties": false
      }
    }
  }
}